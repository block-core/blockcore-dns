{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://+:7010/"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "Dns": {
    "ListenPort": "53",
    "EndServerIp": "192.168.1.1"
  },
  "DnsAgent": {
    "IntervalMin": "1",
    "Hosts": [
      "192.168.1.1"
    ],
    "DidKey":  "[PrivateKey]"
  }
}
